    "numero = int(input(\"Número: \"))\n",
    "multiplicado_por = int(input(\"Multiplicado por: \"))\n",
    "aux = 0\n",
    "\n",
    "for i in range(multiplicado_por):\n",
    "    aux = aux + numero\n",
    "\n",
    "print(\"Resultado: \", aux)"
